; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="The Bouldering Project"
run/main_scene="res://Title/Title.tscn"
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"

[audio]

default_bus_layout=""

[autoload]

State="*res://State.tscn"
Music="*res://Music.tscn"

[display]

window/size/width=160
window/size/height=144
window/size/resizable=false
window/stretch/mode="viewport"

[editor_plugins]

enabled=PoolStringArray( "res://addons/level_loader/plugin.cfg" )

[importer_defaults]

texture={
"flags/filter": false
}

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
quality/dynamic_fonts/use_oversampling=false
